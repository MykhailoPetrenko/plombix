                                    <input type="hidden" name="plombName" value="<%=p.plomb_name%>"/>
                                    <%if(typeof products.items[p.plomb_name]==="undefined"){%>
                                        <input type="submit" value="В корзину"  class="btn">
                                    <%}else{%>
                                        <input type="submit" value="Добавлено"  class="btn btnAdd">
                                    <%}%>
                                </div>
                            </form>
                    </div>
                    <%});%>
                </div>
            </div>
        </section>
