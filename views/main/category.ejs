                                    <!--li(show-tab="tab-company").product-details-navigation__item.tabs-navigation__item.tabs-navigation__item Компании-сотрудники-->
                                </ul>
                            </div>
                        </div>
                        <div class="tabs__body product-details__body">
                            <div class="tab tab-payment tab--active">
                                <div class="content-block">
                                    <h2 class="title-section"><%=main.body.details.payment.head_text%></h2>
                                    <div class="product-table__wrapper"><span class="product-table__title"><%=main.body.details.payment.hea%></span>
                                        <table class="product-table">
                                            <tbody>
                                            <%for(let i=0; i<main.body.details.payment.table_text.length;i++){%>
                                            <tr class="product-table__row">
                                                <td class="product-table__cell"><%=main.body.details.payment.table_text[i]%></td>
                                            </tr>
                                            <%}%>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="tab tab-delivery">
                                <div class="content-block">
                                    <h2 class="title-section"><%=main.body.details.delivery.head_title%></h2>
                                    <p class="text"><%=main.body.details.delivery.head_text%></p>
                                    <div class="product-table__wrapper"><span class="product-table__title"><%=main.body.details.delivery.table_name%></span>
                                        <table class="product-table">
                                            <tbody>
                                            <%for(let i=0; i<main.body.details.delivery.table_text.length;i++){%>
                                            <tr class="product-table__row">
                                                <td class="product-table__cell"><%=main.body.details.delivery.table_text[i]%></td>
                                            </tr>
                                            <%}%>
                                            </tbody>
                                        </table>
                                    </div><span class="product-table__title"><%=main.body.details.delivery.price%></span>
                                    <p class="text"><%=main.body.details.delivery.price_text%></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    <footer class="footer">
        <div class="footer__main">
            <div class="container"><span class="plombix">Plombix</span><span class="plombix__adres"><%=main.footer.address%></span></div>
