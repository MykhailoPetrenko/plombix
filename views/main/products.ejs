                    <%plombs.forEach(function(p){%>
                    <div class="product-prev">
                        <a style="color: black" href="<%=p.link%>">
                        <div class="product-prev__header">
                            <div class="js-product-prev__slider">
                                <%for(key in p.image){%>
                                <%if((p.image[key].match(/(chert.jpg)/g))===null){%>
                                <div class="product-prev__img-wrapper" data-color="<%=p.colors[key]%>"><img class="product-prev__img" src="<%=p.image[key]%>" alt="Изображение товара"></div>
                                <%}%>
                                <%}%>
                            </div>
                        </div>
                        </a>
                        <div class="product-prev__body">
                            <a style="color: black" href="<%=p.link%>">
                            <div class="product-prev__info"><span class="product-prev__title"><%=p.plomb.ru%></span><span class="product-prev__about">Пластик</span></div>
                            <div class="product-prev__price"><span class="product-prev__price-current"><%=p.price.ru%></span><span class="product-prev__price-old">1.8 грн.</span></div>
                            </a>
                            <div class="product-prev__colors"></div>
                        </div>
                        <form class="plomb_form">
                        <div class="product-prev__footer">
                            <input type="hidden" name="plombName" value="<%=p.plomb_name%>"/>
                            <%if(typeof products.items[p.plomb_name]==="undefined"){%>
                                <input type="submit" value="В корзину"  class="btn">
                            <%}else{%>
                                <input type="submit" value="Добавлено"  class="btn btnAdd">
                            <%}%>
                        </div>
                        </form>
                    </div>
                    <%});%>
